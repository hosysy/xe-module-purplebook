<query id="getCountCallbackNumber" action="select">
    <tables>
        <table name="purplebook_callbacks" />
    </tables>
    <columns>
        <column name="COUNT(*)" alias="count" />
    </columns>
    <conditions>
        <condition operation="equal" column="member_srl" var="member_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
