<query id="deleteRecentMessage" action="delete">
	<tables>
		<table name="purplebook_messages" />
	</tables>
	<conditions>
		<condition operation="equal" column="member_srl" var="member_srl" notnull="notnull" />
		<condition operation="equal" column="message_srl" var="message_srl" notnull="notnull" pipe="and" />
	</conditions>
</query>
