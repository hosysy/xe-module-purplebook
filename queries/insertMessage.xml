<query id="insertMessage" action="insert">
	<tables>
		<table name="purplebook_messages" />
	</tables>
	<columns>
		<column name="message_srl" var="message_srl" filter="number" notnull="notnull" />
		<column name="member_srl" var="member_srl" notnull="notnull" />
		<column name="content" var="content" notnull="notnull" />
		<column name="regdate" var="regdate" default="curdate()" />
	</columns>
</query>
